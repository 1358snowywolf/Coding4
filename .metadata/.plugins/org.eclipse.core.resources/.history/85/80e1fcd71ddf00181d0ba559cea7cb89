#include <bits/stdc++.h>

using namespace std;
vector<tuple<int, int, int>> skylines;
set<pair<int, int>> right, height;

void check(int l, int h, int r){
	while(!right.empty() && right.begin()->first < l){
		height.erase(height.find(make_pair(right.begin()->second, right.begin()->first)));
	}
}

int main() {
	int N, M, K;

	while(cin >> N){
		cin >> M >> K;
		skylines.push_back(make_tuple(N, M, K));
	}

	sort(skylines.begin(), skylines.end());

	for(auto S : skylines){
		int l, h, r;
		tie(l, h, r) = S;
		check(l, h, r);
	}

	return 0;
}
