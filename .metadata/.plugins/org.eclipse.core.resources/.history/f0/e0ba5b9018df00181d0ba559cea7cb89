#include <bits/stdc++.h>

using namespace std;

int main() {
	vector<tuple<int, int, int>> skyline;
	int start, height, end;
	set<pair<int, int>> heights;
	set<pair<int, int>> rights;

	while(cin >> start){
		cin >> height >> end;
		skyline.push_back(make_tuple(start, height, end));
	}

	sort(skyline.begin(), skyline.end());

	for(auto triple : skyline){
		int left, height, right;
		tie(left, height, right) = triple;

		rights.insert(make_pair(right, height));
		heights.insert(make_pair(height, right));
		newHeight = heights.begin()->first;
	}

	return 0;
}
