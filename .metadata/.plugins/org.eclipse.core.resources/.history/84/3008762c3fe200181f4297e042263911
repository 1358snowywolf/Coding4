#include <bits/stdc++.h>

using namespace std;

map<int, vector<pair<int, int>>> makeGraph(vector<vector<int>>){

}

int main() {
	int N, K;

	while(cin >> N){
		cin >> K;
		vector<vector<int>> elevators;
		vector<int> speed;

		for(int i = 0; i < N; i++){
			int s;
			cin >> s;
			speed.push_back(s);
		}

		for(int i = 0; i < N; i++){
			int tmp;
			string line;
			getline(cin, line);
			istringstream iss(line);
			vector<int> input;

			while(iss >> tmp){
				input.push_back(tmp);
			}

			elevators.push_back(input);
		}



	}

	return 0;
}
