#include <bits/stdc++.h>

using namespace std;
map<string, vector<pair<string, int>>> graph;

int main() {
	int N;
	while(cin >> N){
		graph.clear();
		string start, end;
		cin >> start >> end;

		for(int i = 0; i < N; i++){
			string u, v, word;
			cin >> u >> v >> word;

			if(graph.find(u) == graph.end()){
				graph.insert(make_pair(u, vector<pair<string, int>> ()));
			}
			if(graph.find(v) == graph.end()){
				graph.insert(make_pair(v, vector<pair<string, int>> ()));
			}

			graph[u].push_back(make_pair(v, word.size()));
			graph[v].push_back(make_pair(u, word.size()));
		}

	}
	return 0;
}
