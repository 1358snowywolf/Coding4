#include <bits/stdc++.h>

using namespace std;

int main() {
	int C;
	cin >> C;

	for(int i = 0; i < C; i++){
		int N, M;
		cin >> N >> M;
		vector<tuple<int, int, int>> edges;

		for(int j = 0; j < M; j++){
			int start, end, weight;
			cin >> start >> end >> weight;
			edges.push_back(make_tuple(start, end, weight));
		}

	}
	return 0;
}
