#include <bits/stdc++.h>

using namespace std;
map<int, vector<pair<int, int>>> graph;

int main(){
	int C;
	cin >> C;

	for(int i = 0; i < C; i++){
		graph.clear();
		int N, E, T, M;
		cin >> N >> E >> T >> M;

		for(int i = 0; i < N; i++){
			graph.insert(make_pair(i, vector<pair<int, int>> ()));
		}

		for(int i = 0; i < M; i++){
			int u, v, weight;
			cin >> u >> v >> weight;
			graph[u - 1].push_back(make_pair(v, weight));
		}

	}


	return 0;
}
