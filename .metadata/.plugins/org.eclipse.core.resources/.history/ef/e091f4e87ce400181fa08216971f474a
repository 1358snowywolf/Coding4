#include <bits/stdc++.h>

using namespace std;

int main() {
	int N;

	while(cin >> N){
		string start, end;
		cin >> start >> end;
		map<string, vector<string>> languages;

		for(int i = 0; i < N; i++){
			string u, v, w;
			cin >> u >> v >> w;

			if(languages.find(u) == languages.end()){
				languages.insert(make_pair(u, vector<string> ()));
			}

			langauges[u].push_back(v);
		}
	}
	return 0;
}
