#include <bits/stdc++.h>

using namespace std;

int main() {
	int N, K;

	while(cin >> N){
		cin >> K;
		map<int, vector<int>> elevators;
		vector<int> speed;

		for(int i = 0; i < N; i++){
			int s;
			cin >> s;
			speed.push_back(s);
		}

		for(int i = 0; i < N; i++){
			string line;
			getline(cin, line);
			istringstream iss(line);
			elevators.insert(make_pair(i * 100, vector<int> ()));

			while(line >> iss){
				elevators[i * 100].push_back(stoi(iss));
			}
		}
	}

	return 0;
}
